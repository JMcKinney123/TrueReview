
<table class="table">

    <tr>
        <th>Title</th>
        <th>AboutMe</th>
        <th>Reviews</th>
        <th>Favorite Genre</th>

    </tr>

    @foreach (var profile in Model)
    {
        <tr>
            <td>@profile.Title</td>
            <td>@profile.AboutMe</td>
            <td>@profile.Reviews</td>
            <td>@profile.GenreName</td>
        </tr>
    }
</table>


